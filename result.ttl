mothur:pairwise.seqs a ngsp:Module ;
    ngsp:outputPattern [ a cnt:Chars ;
            ngsp:parameterName "type" ;
            ngsp:pattern [ ngsp:patternString "[filename],[outputtag],dist" ;
                    dc:identifier "phylip" ],
                [ ngsp:patternString "[filename],dist" ;
                    dc:identifier "column" ] ;
            cnt:chars """getOutputPattern(string type) {
    try {
        string pattern = "";
        
        if (type == "phylip") {  pattern = "[filename],[outputtag],dist"; } 
        else if (type == "column") { pattern = "[filename],dist"; }
        else { m->mothurOut("[ERROR]: No definition for type " + type + " output pattern.\\n"); m->setControl_pressed(true);  }
        
