                    dc:identifier "list" ],
                [ ngsp:patternString "[filename],pick,[extension]" ;
                    dc:identifier "count" ],
                [ ngsp:patternString "[filename],pick,[extension]" ;
                    dc:identifier "name" ],
                [ ngsp:patternString "[filename],pick,[extension]" ;
                    dc:identifier "fasta" ],
                [ ngsp:patternString "[filename],pick,[extension]" ;
                    dc:identifier "taxonomy" ],
                [ ngsp:patternString "[filename],pick,[extension]" ;
                    dc:identifier "qfile" ],
                [ ngsp:patternString "[filename],pick.accnos.report" ;
                    dc:identifier "accnosreport" ],
