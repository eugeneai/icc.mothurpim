}""" ] ;
    ngsp:parameter mothur:pairwise.seqs-align-parameter,
        mothur:pairwise.seqs-calc-parameter,
        mothur:pairwise.seqs-compress-parameter,
        mothur:pairwise.seqs-countends-parameter,
        mothur:pairwise.seqs-cutoff-parameter,
        mothur:pairwise.seqs-fasta-parameter,
        mothur:pairwise.seqs-gapextend-parameter,
