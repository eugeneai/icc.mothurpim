mothur:classify.seqs a ngsp:Module ;
    ngsp:outputPattern [ a cnt:Chars ;
            ngsp:parameterName "type" ;
            ngsp:pattern [ ngsp:patternString "[filename],[tag],[tag2],tax.summary" ;
                    dc:identifier "taxsummary" ],
                [ ngsp:patternString "[filename],[tag],[tag2],match.dist" ;
                    dc:identifier "matchdist" ],
                [ ngsp:patternString "[filename],[tag],[tag2],flip.accnos" ;
                    dc:identifier "accnos" ],
                [ ngsp:patternString "[filename],[tag],[tag2],taxonomy" ;
                    dc:identifier "taxonomy" ] ;
            cnt:chars """getOutputPattern(string type) {
    try {
        string pattern = "";
        
        if (type == "taxonomy") {  pattern = "[filename],[tag],[tag2],taxonomy"; } 
        else if (type == "taxsummary") {  pattern = "[filename],[tag],[tag2],tax.summary"; } 
        else if (type == "accnos") {  pattern =  "[filename],[tag],[tag2],flip.accnos"; }
        else if (type == "matchdist") {  pattern =  "[filename],[tag],[tag2],match.dist"; }
        else { m->mothurOut("[ERROR]: No definition for type " + type + " output pattern.\\n"); m->setControl_pressed(true);  }
        
        return pattern;
    }
    catch(exception& e) {
        m->errorOut(e, "ClassifySeqsCommand", "getOutputPattern");
        exit(1);
    }
}""" ] ;
