        mothur:get.oturep-method-parameter,
        mothur:get.oturep-name-parameter,
        mothur:get.oturep-outputdir-parameter,
        mothur:get.oturep-phylip-parameter,
        mothur:get.oturep-precision-parameter,
        mothur:get.oturep-seed-parameter,
        mothur:get.oturep-sorted-parameter,
