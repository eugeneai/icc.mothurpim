mothur:mgcluster a ngsp:Module ;
    ngsp:outputPattern [ a cnt:Chars ;
            ngsp:parameterName "type" ;
            ngsp:pattern [ ngsp:patternString "[filename],[clustertag],rabund" ;
                    dc:identifier "rabund" ],
                [ ngsp:patternString "[filename],[clustertag],steps" ;
                    dc:identifier "steps" ],
                [ ngsp:patternString "[filename],[clustertag],list-[filename],[clustertag],[tag2],list" ;
                    dc:identifier "list" ],
                [ ngsp:patternString "[filename],[clustertag],sensspec" ;
                    dc:identifier "sensspec" ],
                [ ngsp:patternString "[filename],[clustertag],sabund" ;
                    dc:identifier "sabund" ] ;
            cnt:chars """getOutputPattern(string type) {
    try {
        string pattern = "";
        
        if (type == "list") {  pattern = "[filename],[clustertag],list-[filename],[clustertag],[tag2],list"; } 
        else if (type == "rabund") {  pattern = "[filename],[clustertag],rabund"; } 
        else if (type == "sabund") {  pattern = "[filename],[clustertag],sabund"; }
        else if (type == "steps") {  pattern = "[filename],[clustertag],steps"; }
        else if (type == "sensspec") {  pattern = "[filename],[clustertag],sensspec"; }
        else { m->mothurOut("[ERROR]: No definition for type " + type + " output pattern.\\n"); m->setControl_pressed(true);  }
        
        return pattern;
    }
    catch(exception& e) {
        m->errorOut(e, "MGClusterCommand", "getOutputPattern");
        exit(1);
    }
}""" ] ;
    ngsp:parameter mothur:mgcluster-adjust-parameter,
