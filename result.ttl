mothur:align.check a ngsp:Module ;
    ngsp:outputPattern [ a cnt:Chars ;
            ngsp:parameterName "type" ;
            ngsp:pattern [ ngsp:patternString "[filename],align.check" ;
                    dc:identifier "aligncheck" ] ;
            cnt:chars """getOutputPattern(string type) {
    try {
        string pattern = "";
        
        if (type == "aligncheck") {  pattern = "[filename],align.check"; } 
        else { m->mothurOut("[ERROR]: No definition for type " + type + " output pattern.\\n"); m->setControl_pressed(true);  }
        
        return pattern;
    }
    catch(exception& e) {
        m->errorOut(e, "AlignCheckCommand", "getOutputPattern");
        exit(1);
    }
}""" ] ;
    ngsp:parameter mothur:align.check-count-parameter,
        mothur:align.check-fasta-parameter,
        mothur:align.check-inputdir-parameter,
        mothur:align.check-map-parameter,
        mothur:align.check-name-parameter,
        mothur:align.check-outputdir-parameter,
        mothur:align.check-seed-parameter ;
    dc:title "align.check" ;
    dcterms:description "calculate the number of potentially misaligned bases in a 16S rRNA gene sequence alignment" ;
    schema:citation "http://www.mothur.org/wiki/Align.check" ;
    schema:softwareHelp """The align.check command reads a fasta file and map file as well as an optional name or count file.
It outputs a file containing the secondary structure matches in the .align.check file.
The align.check command parameters are fasta and map, both are required.
The align.check command should be in the following format: align.check(fasta=yourFasta, map=yourMap).
Example align.check(map=silva.ss.map, fasta=amazon.fasta).""" ;
    nco:websiteURL <http://www.mothur.org/wiki/Align.check> ;
    v:category "Sequence Processing" .

