mothur:get.sharedseqs a ngsp:Module ;
    ngsp:outputPattern [ a cnt:Chars ;
            ngsp:parameterName "type" ;
            ngsp:pattern [ ngsp:patternString "[filename],[distance],[group],shared.fasta" ;
                    dc:identifier "fasta" ],
