        mothur:heatmap.bin-groups-parameter,
        mothur:heatmap.bin-inputdir-parameter,
        mothur:heatmap.bin-label-parameter,
        mothur:heatmap.bin-list-parameter,
        mothur:heatmap.bin-numotu-parameter,
        mothur:heatmap.bin-outputdir-parameter,
